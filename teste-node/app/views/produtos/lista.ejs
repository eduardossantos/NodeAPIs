<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
    </head>
    <body>
        <table>
            <tr>
                <td>id</td>
                <td>Título do Livro</td>
                <td>Descricão</td>
                <td>Preço</td>
            </tr>
            <tr>
                <%for(var i=0; i<lista.length; i++){%>
                  <tr>
                      <td><%=lista[i].id%></td>
                      <td><%=lista[i].titulo%></td>
                      <td><%=lista[i].descricao%></td>
                      <td><%="R$ "+lista[i].preco%></td>
                  </tr>
                <%}%>
            </tr>
        </table>

    </body>
</html>